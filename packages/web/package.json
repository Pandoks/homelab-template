{
  "name": "@startup-template/web",
  "version": "0.0.0",
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "docker compose -f ./compose.yaml up -d && sleep 1 && drizzle-kit push --force && dotenv vite dev",
    "build": "vite build",
    "build:test": "NODE_ENV=test vite build --mode test",
    "preview": "vite preview",
    "test": "npm run test:int && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:gen": "playwright codegen",
    "test:int": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:unit": "vitest",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.2.2",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^5.2.0",
    "@types/highlightjs": "^9.12.6",
    "@types/node": "^20.14.9",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.4",
    "drizzle-kit": "^0.24.2",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^2.0.4"
  },
  "type": "module",
  "dependencies": {
    "@node-rs/argon2": "^1.8.3",
    "@oslojs/crypto": "^0.6.0",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.0.0",
    "@oslojs/webauthn": "^0.6.0",
    "@startup-template/core": "workspace:^",
    "bits-ui": "^0.21.13",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "formsnap": "^1.0.1",
    "highlight.js": "^11.10.0",
    "lucide-svelte": "^0.416.0",
    "postgres": "^3.4.4",
    "redis": "^4.6.15",
    "sveltekit-superforms": "^2.15.2",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "uqr": "^0.1.2",
    "zod": "^3.23.8"
  }
}
