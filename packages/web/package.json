{
  "name": "@startup-template/web",
  "version": "0.0.0",
  "files": [
    "build"
  ],
  "scripts": {
    "setup": "pnpm run reset && docker compose -f ../core/compose.yaml -p setup up -d",
    "reset": "docker compose -p setup down",
    "dev": "pnpm run setup && dotenv -e ../../.env -- vite dev",
    "build": "vite build",
    "test:unit": "pnpm run setup && vitest",
    "test:int": "if [ -z \"$CI\" ]; then dotenv -e ../../.env -- playwright test; else playwright test; fi",
    "test:gen": "dotenv -e ../../.env -- playwright codegen",
    "test:ui": "dotenv -e ../../.env -- playwright test --ui",
    "lint": "svelte-check",
    "format": "prettier --write .",
    "prepare": "svelte-kit sync"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.8.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.1",
    "@testing-library/svelte": "^5.2.6",
    "@types/highlightjs": "^9.12.6",
    "@types/node": "^22.9.3",
    "autoprefixer": "^10.4.19",
    "bits-ui": "1.0.0-next.64",
    "dotenv-cli": "^7.4.4",
    "formsnap": "2.0.0-next.1",
    "lucide-svelte": "^0.460.1",
    "postcss": "^8.4.49",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.3.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "svelte": "^5.2.7",
    "svelte-check": "^4.1.0",
    "sveltekit-superforms": "^2.20.1",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "zod": "^3.23.8"
  },
  "type": "module",
  "dependencies": {
    "@node-rs/argon2": "^2.0.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.0.0",
    "@oslojs/webauthn": "^1.0.0",
    "@startup-template/core": "workspace:^",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "highlight.js": "^11.10.0",
    "postgres": "^3.4.5",
    "redis": "^4.6.15",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.3.0",
    "uqr": "^0.1.2"
  }
}
