{
  "name": "@startup-template/core",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "db:reset": "rm -rf ./src/database/main/dev/meta ./src/database/main/dev/_init.sql",
    "db:gen": "pnpm run db:reset && drizzle-kit generate --config ./src/database/main/drizzle.config.ts --name init",
    "test:unit": "pnpm run setup && sst shell vitest"
  },
  "exports": {
    "./*": "./src/*.ts"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/webauthn": "^1.0.0",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "postgres": "^3.4.5",
    "redis": "^4.6.15",
    "sst": "^3.3.27"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.28.1",
    "vitest": "^2.1.5"
  }
}
