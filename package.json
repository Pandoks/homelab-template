{
  "name": "startup-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "NODE_ENV=test playwright test",
    "test:unit": "vitest",
    "testdb:push": "drizzle-kit push --config=drizzle-test.config.ts",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:drop": "drizzle-kit drop",
    "db:update": "drizzle-kit up",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^5.2.0",
    "@types/highlightjs": "^9.12.6",
    "@types/node": "^20.14.9",
    "@types/qrcode-generator": "^1.0.6",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.23.0",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^2.0.4"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@node-rs/argon2": "^1.8.3",
    "@oslojs/crypto": "^0.6.0",
    "@oslojs/webauthn": "^0.6.0",
    "bits-ui": "^0.21.12",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.32.0",
    "drizzle-zod": "^0.5.1",
    "formsnap": "^1.0.1",
    "highlight.js": "^11.10.0",
    "lucia": "^3.2.0",
    "lucide-svelte": "^0.416.0",
    "oslo": "^1.2.1",
    "postgres": "^3.4.4",
    "qrcode-generator": "^1.4.4",
    "redis": "^4.6.15",
    "sveltekit-superforms": "^2.15.2",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8"
  }
}
